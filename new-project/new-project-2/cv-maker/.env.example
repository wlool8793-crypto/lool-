# Application Configuration
VITE_APP_NAME="CV Maker Pro"
VITE_APP_VERSION="2.0.0"
VITE_APP_DESCRIPTION="Professional CV and Marriage Biodata Maker"

# API Configuration (for future use)
VITE_API_BASE_URL=""
VITE_API_TIMEOUT=10000

# File Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_IMAGE_TYPES="image/jpeg,image/png,image/jpg,image/webp"
VITE_MAX_IMAGE_WIDTH=1200
VITE_MAX_IMAGE_HEIGHT=1200

# PDF Configuration
VITE_PDF_QUALITY=0.95
VITE_PDF_PAGE_MARGINS="20mm"
VITE_PDF_DPI=300

# Image Cropping Configuration
VITE_CROP_ASPECT_RATIO_FREE="free"
VITE_CROP_ASPECT_RATIO_SQUARE="1:1"
VITE_CROP_ASPECT_RATIO_43="4:3"
VITE_CROP_ASPECT_RATIO_34="3:4"

# Default Language
VITE_DEFAULT_LANGUAGE="en"
VITE_SUPPORTED_LANGUAGES="en,hi,ur,ar,fr,es,de,zh,ja,pt"

# Feature Flags
VITE_ENABLE_MARRIAGE_MODULE=true
VITE_ENABLE_MULTI_LANGUAGE=true
VITE_ENABLE_IMAGE_CROPPING=true
VITE_ENABLE_PDF_PROTECTION=true
VITE_ENABLE_EXPORT_OPTIONS=true

# Analytics (for future use)
VITE_ENABLE_ANALYTICS=false
VITE_ANALYTICS_ID=""

# Development
VITE_DEV_MODE=true
VITE_SHOW_DEV_TOOLS=false